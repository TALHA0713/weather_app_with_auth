<button><a href="/user/create">Add-User</a></button>

  <div class="blogs content">
    <h2>All Users</h2>

    <% if (users.length > 0) { %>
      <% users.forEach(eachuser => { %>
        <a class="single" href="/users/<%= eachuser.id %>">
          <h3 class="title"><%= eachuser.name %></h3>
          <p class="snippet"><%= eachuser.email%></p>
        </a>
      <% }) %>
    <% } else { %>
      <p>There are no users to display...</p>
    <% } %>
    
  </div>

 